{
  "name": "auth-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf .build && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "db:studio": "prisma studio --schema './database/schema.prisma'",
    "db:migrations": "prisma migrate dev --schema './database/schema.prisma' --create-only --name",
    "db:migrate": "prisma migrate dev --schema './database/schema.prisma'",
    "prisma:sync": "prisma generate --schema './database/schema.prisma'",
    "prepare": "husky install",
    "prisma:prod:deploy": "prisma migrate deploy --schema './database/schema.prisma'",
    "prestart:prod": "npm run prisma:prod:deploy",
    "start:prod": "node .build/server"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^18.8.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
